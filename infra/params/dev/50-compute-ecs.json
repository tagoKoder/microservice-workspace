{
  "Env": "dev",
  "ProjectName": "imaginarybank",
  "DomainName": "tudominio.com",
  "HostedZoneId": "ZXXXXXXXXXXXXX",
  "ApiSubdomain": "api",
  "AcmCertArnApi": "arn:aws:acm:us-east-1:123456789012:certificate/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
  "AlbSslPolicy": "ELBSecurityPolicy-TLS13-1-2-2021-06",

  "BffImageUri": "123456789012.dkr.ecr.us-east-1.amazonaws.com/imaginarybank-dev-bff:REEMPLAZA_TAG",
  "BffContainerPort": 8080,
  "BffDesiredCount": 1,
  "BffCpu": 256,
  "BffMemory": 512,
  "HealthCheckPath": "/health",
  "LogRetentionDays": 14,
  "EnableContainerInsights": "false",

  "NetworkStackName": "imaginarybank-dev-10-network",

  "ApiDomainName": "api.tudominio.com",

  "IdentityImageUri": "ACCOUNT.dkr.ecr.REGION.amazonaws.com/identity:dev-latest",
  "AccountImageUri": "ACCOUNT.dkr.ecr.REGION.amazonaws.com/account:dev-latest",
  "LedgerImageUri": "ACCOUNT.dkr.ecr.REGION.amazonaws.com/ledger:dev-latest",

  "IdentityGrpcPort": 7001,
  "AccountGrpcPort": 7002,
  "LedgerGrpcPort": 7003,

  "IdentityDesiredCount": 1,
  "AccountDesiredCount": 1,
  "LedgerDesiredCount": 1,

  "IdentityDbSecretArn": "arn:aws:secretsmanager:REGION:ACCOUNT:secret:bank/dev/db/identity-xxxxx",
  "AccountDbSecretArn": "arn:aws:secretsmanager:REGION:ACCOUNT:secret:bank/dev/db/account-xxxxx",
  "LedgerDbSecretArn": "arn:aws:secretsmanager:REGION:ACCOUNT:secret:bank/dev/db/ledger-xxxxx"
}
