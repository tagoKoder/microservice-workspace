    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Clinic",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/clinic/cmd/main.go",
            "envFile": "${workspaceFolder}/clinic/.env",
            "output": "${workspaceFolder}/.vscode/.debug/clinic/.debug_bin"
        },
        {
            "name": "Debug Gateway",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/gateway/cmd/main.go",
            "envFile": "${workspaceFolder}/gateway/.env",
            "output": "${workspaceFolder}/.vscode/.debug/gateway/.debug_bin"
        },
    ]
}